#!/usr/bin/env sh

. "$HOME/.config/etc/base.sh"

cd

"$HOME/.config/bin/rm-ds-store" .

[ -e Applications ] \
    && [ -e Applications/.localized ] \
    && [ "$(find Applications | wc -l)" -le 2 ] \
    && rm -rv Applications

rm -rfv \
    .bash_history \
    .bundle \
    .gem \
    .ghc \
    .lldb \
    .node_repl_history \
    .npm \
    .oracle_jre_usage \
    .python_history \
    .sh_history \
    .viminfo
