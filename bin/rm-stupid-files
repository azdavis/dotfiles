#!/bin/sh

. "$HOME/.config/lib/base.sh"

cd

"$HOME/.config/bin/rm-ds-store" .

[ -e Applications ] \
    && [ "$(find Applications -not -name .localized | wc -l)" -eq 1 ] \
    && rm -rv Applications

rm -rfv \
    .bash_history \
    .bash_sessions \
    .bundle \
    .elm \
    .gem \
    .ghc \
    .lldb \
    .node-gyp \
    .node_repl_history \
    .npm \
    .oracle_jre_usage \
    .python_history \
    .sh_history \
    .viminfo
