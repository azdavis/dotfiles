#!/bin/sh

set -efu
IFS=

. $HOME/.config/lib/panic.sh

if [ $# -ne 0 ]; then
    panic "usage: $(basename $0)"
fi

cd

find . -name .DS_Store -delete >/dev/null 2>&1 || true

rm -rf \
    .bash_history \
    .bash_sessions \
    .config/Code/Backups \
    .config/Code/Cache \
    .config/Code/CachedData \
    .config/Code/CachedExtensions \
    .config/Code/GPUCache \
    .config/Code/logs \
    .config/Code/storage.json \
    .gdb_history \
    .idris/repl/history \
    .node_repl_history \
    .oracle_jre_usage \
    .python_history \
    .sh_history \
    .sqlite_history \
    .viminfo \
    .zcompdump
