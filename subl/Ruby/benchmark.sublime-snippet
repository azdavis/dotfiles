  <content><![CDATA[
require 'benchmark'

TIMES = 100_000

a = nil
b = nil

Benchmark.bmbm do |bm|
  bm.report 'a' do
    TIMES.times do
      a = 'a'
    end
  end
  bm.report 'b' do
    TIMES.times do
      b = 'b'
    end
  end
end

puts a == b

]]></content>
  <tabTrigger>bm</tabTrigger>
  <scope>source.ruby</scope>
  <description>rb</description>
</snippet>
