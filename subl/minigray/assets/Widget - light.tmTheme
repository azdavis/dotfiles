<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>settings</key>
    <array>
        <!-- settings -->
        <dict>
            <key>settings</key>
            <dict>
                <key>background</key>
                <string>#eee</string>
                <key>caret</key>
                <string>#222</string>
                <key>foreground</key>
                <string>#222</string>
                <key>selection</key>
                <string>#ccc</string>
            </dict>
        </dict>
        <!-- black and red -->
        <dict>
            <key>scope</key>
            <string>invalid</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#000</string>
                <key>background</key>
                <string>#f00</string>
            </dict>
        </dict>
        <!-- red -->
        <dict>
            <key>scope</key>
            <string>keyword.operator</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#900</string>
            </dict>
        </dict>
        <!-- green -->
        <dict>
            <key>scope</key>
            <string>keyword.control</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#090</string>
            </dict>
        </dict>
        <!-- cyan -->
        <dict>
            <key>scope</key>
            <string>keyword.control.group, keyword.other.backref-and-recursion</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#099</string>
            </dict>
        </dict>
        <!-- blue -->
        <dict>
            <key>scope</key>
            <string>constant.character</string>
            <key>settings</key>
            <dict>
                <key>foreground</key>
                <string>#009</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
